{
    "name": " Khludev/KuLapAdminPanel",
    "description": "A KuLapAdminPanel for Laravel 6. Forked from Wikichua/simplecontrolpanel",
    "license": "MIT",
    "authors": [
        {
            "name": "Khludev",
            "email": "khludev@gmail.com",
            "homepage": "https://github.com/Khludev",
            "role": "Forker"
        }
    ],
    "homepage": "https://github.com/Khludev/KuLapAdminPanel",
    "keywords": ["Laravel", "KuLapAdminPanel"],
    "require": {
        "erusev/parsedown": "^1.7",
        "yajra/laravel-datatables-oracle": "^9.6",
        "tightenco/ziggy": "^0.8.0",
        "kalnoy/nestedset": "^5.0",
        "justbetter/laravel-pagination-with-havings": "^1.2",
        "laravel/helpers": "^1.1",
        "artesaos/seotools": "^0.16.0",
        "yajra/laravel-datatables": "^1.5",
        "phpoffice/phpspreadsheet": "^1.9",
        "rap2hpoutre/fast-excel": "^1.3",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "pusher/pusher-php-server": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Khludev\\Simplecontrolpanel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Khludev\\Simplecontrolpanel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khludev\\KuLapAdminPanel\\SimpleControlPanelServiceProvider",
                "Khludev\\KuLapAdminPanel\\WidgetServiceProvider"
            ],
            "aliases": {
                "SimpleControlPanel": "Khludev\\KuLapAdminPanel\\Facades\\SimpleControlPanel",
                "Widget": "Khludev\\KuLapAdminPanel\\Facades\\Widget"
            }
        }
    }
}
